{
    "name": "to-phuoc-day/quanlydoan",
    "description": "He thong quan ly do an nien luan (MVC PHP)",
    "type": "project",
    "require": {
        "php": ">=7.4",
        "ext-pdo_mysql": "*",
        "ezyang/htmlpurifier": "^4.18",
        "phpmailer/phpmailer": "^7.0",
        "vlucas/phpdotenv": "^5.4"
    },
    "autoload": {
        "psr-4": {
            "App\\": [
                "models/",
                "controllers/"
            ]
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "authors": [
        {
            "name": "To Phuoc Day",
            "email": "day@example.com"
        }
    ]
}
